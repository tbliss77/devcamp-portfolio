<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "Tyson Bliss' Portfolio", sticky: false) %>

  <% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], title: "Tyson Bliss' Portfolio", sticky: false) %>

  <% else %>
  <%= js add_gritter(flash[:notice], title: "Tyson Bliss' Portfolio", sticky: false) %>
<% end %>